  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "conc pnpm:build:code pnpm:build:declaration",
    "postbuild": "publint",
    "build:code": "tsc --project tsconfig.build.json --removeComments",
    "build:declaration": "tsc --project tsconfig.build.json --declaration --declarationMap --emitDeclarationOnly",
    "build:diagrams": "plantuml docs/*.puml",
    "build:docs": "typedoc --validation",
    "ci": "conc pnpm:build pnpm:lint pnpm:test",
    "cost": "bun build --minify --target bun src/index.js | gzip | wc -c",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "lc": "find dist -type f -name \"*.js\" -exec cat {} \\; | wc -l",
    "lint": "conc pnpm:lint:*",
    "lint:eslint": "eslint .",
    "lint:typedoc": "typedoc --validation --emit none",
    "prepublishOnly": "pnpm run ci",
    "start": "conc -k pnpm:start:*",
    "start:code": "pnpm run build:code -- --watch --preserveWatchOutput ",
    "start:declaration": "pnpm run build:declaration -- --watch --preserveWatchOutput",
    "test": "vitest run",
    "watch": "conc -k pnpm:watch:*",
    "watch:code": "pnpm run start:code -- --noEmit",
    "watch:declaration": "pnpm run start:declaration -- --noEmit"
  },
  "dependencies": {
    "bech32": "2.0.0",
    "content-type": "1.0.5",
    "valibot": "^1.2.0",
    "varint": "6.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@bitcoinerlab/secp256k1": "^1.2.0",
    "@eslint/js": "^9.39.2",
    "@noble/secp256k1": "^2.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/content-type": "^1.1.9",
    "@types/mime-db": "^1.43.6",
    "@types/varint": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "better-sqlite3": "^12.5.0",
    "bip32": "^5.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fake-indexeddb": "^6.2.5",
    "mime-db": "^1.54.0",
    "prettier": "^3.7.4",
    "prettier-plugin-jsdoc": "^1.8.0",
    "publint": "^0.3.16",
    "typedoc": "^0.28.15",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^6.4.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@bitcoinerlab/secp256k1": "^1.0.0",
    "@noble/secp256k1": "^2.0.0",
    "better-sqlite3": "*",
    "bip32": "^5.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@bitcoinerlab/secp256k1": {
      "optional": true
    },
    "@noble/secp256k1": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "bip32": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  }
}
