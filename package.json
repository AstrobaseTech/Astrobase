    },
    "./bip39/wordlist/english": {
      "default": "./dist/bip39/wordlist/en.js"
    },
    "./cid": {
      "types": "./dist/cid/cid.d.ts",
      "default": "./dist/cid/cid.js"
    },
    "./codecs": {
      "types": "./dist/codecs/codecs.d.ts",
      "default": "./dist/codecs/codecs.js"
    },
