    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "default": "./dist/middleware/index.js"
    },
    "./mutable": {
      "types": "./dist/mutable/mutable.d.ts",
      "default": "./dist/mutable/mutable.js"
    },
    "./rpc": {
      "types": "./dist/rpc/shared/index.d.ts",
      "default": "./dist/rpc/shared/index.js"
